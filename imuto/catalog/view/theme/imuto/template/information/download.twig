{{ header }}
<div id="information-information" class="container-fluid content-container">
  <div class="row breadcrumb-container">
    <div class="container">
      <div class="row">
        <div class="col-xs-12">
          <ul class="breadcrumb">
            {% for breadcrumb in breadcrumbs %}
              <li class="font14"><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
            {% endfor %}
          </ul>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="container">
      <div class="row">
        {% for downloads in download %}
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 ">
          <div class="down">
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-6 down-img">
                <img class="" src="{{ downloads.image }}">
            </div>
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-6 down-content ">
                <p class="font20">{{ downloads.title}}</p>
                <h4 >{{ text_content }}:</h3>
                <p>{{ downloads.content }}</p>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-6 download"><a class="btn" target="_blank" href="{{ downloads.href }}">{{ text_downloads }}</a></div>
          </div>
        </div>  
        {% endfor %}

      </div>
    </div>
  </div>
</div>
{{ footer }}