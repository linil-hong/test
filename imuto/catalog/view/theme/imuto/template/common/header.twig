<body>
<div class="main-container">
  <nav id="top">
    <div class="container">
      {% if logo %}
        <div id="logo" style="z-index:999">
          <a href="{{ home }}"><img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-responsive" /></a>
        </div>
      {% endif %}
      <div class="pull-right">
        <div class="pull-left hidden-xs">
          <a class="top-btn active" href="http://photoolex.net/" target="_blank" title="{{ text_photoolex }}">Photoolex</a>
        </div>
        {{ currency }}
        {{ language }}
        <div id="top-links" class="nav nopadding">
          <ul class="list-inline nomargin">
            {% if logged %}
            <li class="dropdown"><a style="width:30px;height:30px;border-radius:50%;background-color: #753678;" href="{{ account }}" title="{{ text_account }}" class="dropdown-toggle parent-btn" data-toggle="dropdown"><i class="top-menu-icon icon-login"></i></a>
              <ul class="dropdown-menu">
                  <li><a href="{{ account }}">{{ text_account }}</a></li>
                  <li><a href="{{ order }}">{{ text_order }}</a></li>
                  <li><a href="{{ logout }}">{{ text_logout }}</a></li>
              </ul>
            </li>
            {% else %}
            <li class="dropdown"><a href="{{ account }}" title="{{ text_account }}" class="dropdown-toggle parent-btn" data-toggle="dropdown"><i class="top-menu-icon icon-login"></i></a>
              <ul class="dropdown-menu">
                  <li><a href="{{ register }}">{{ text_register }}</a></li>
                  <li><a href="{{ login }}">{{ text_login }}</a></li>
              </ul>
            </li>
            {% endif %}
            <li><a class="parent-btn" href="{{ product_search }}" title="{{ text_search }}"><i class="top-menu-icon icon-search"></i></a></li>
            <li><a class="parent-btn" href="http://www.imuto.com/im_bbs/" title="{{ text_user_forum }}" target="_blank"><i class="top-menu-icon icon-im"></i></a></li>
            <li><a id="shopping_cart" class="parent-btn" href="{{shopping_cart}}" title="{{ text_shopping_cart }}"><i class="top-menu-icon icon-cart"></i>
              {% if num %}
              <i class="top-menu-num">{{ num }}</i>
              {% else %}
              <i></i>
              {% endif %}
            </a></li>
          </ul>
        </div>
      </div>
    </div>
  </nav>
{{ menu }}
